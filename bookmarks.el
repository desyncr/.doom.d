;;;; Emacs Bookmark Format Version 1;;;; -*- coding: utf-8-emacs; mode: lisp-data -*-
;;; This format is meant to be slightly human-readable;
;;; nevertheless, you probably don't want to edit it.
;;; -*- End Of Bookmark File Format Version Stamp -*-
(;; Personal bookmarks
 ("Personal > TODOs"
  (filename . "~/Personal/todo.org"))
 ("Personal > Emacs"
  (filename . "~/Personal/emacs.org"))
("Personal > Projects"
  (filename . "~/Personal/projects.org"))
("Personal > Journal"
  (filename . "~/Personal/journal.org"))
;; Work bookmarks
("Work > TODOs"
  (filename . "~/Work/tasks.org"))
("Work > Commands"
  (filename . "~/Work/notes/commands.org"))
;; Config bookmarks
("Doom > Config"
  (filename . "~/.config/doom/Emacs.org"))
("Doom > Bookmarks"
  (filename . "~/.config/doom/bookmarks.el"))
("Doom > Packages"
  (filename . "~/.config/doom/config/packages.org"))
("Doom > Init.el"
  (filename . "~/.config/doom/init.el")))
